<center><h3> Edit User: </h3></center>
<% if @tenant %>
    <%= form_for @tenant do |f| %>
        <%= f.label "First Name" %>
        <%= f.text_field :first_name %>
        <br>
        <%= f.label "Last_name" %>
        <%= f.text_field :last_name %>
        <br>
        <%= f.label :age %>
        <%= f.text_field :age %>
        <br>
        <%= f.label :about %>
        <%= f.text_area :about %>

        <%= f.submit "Save Edit"  %>
        <% end %>
<% else %>
    <%= form_for @landlord do |f| %>
        <%= f.label "First Name" %>
        <%= f.text_field :first_name %>
        <br>
        <%= f.label "Last_name" %>
        <%= f.text_field :last_name %>
        <br>
        <%= f.label :age %>
        <%= f.text_field :age %>
        <br>
        <%= f.label :about %>
        <%= f.text_area :about %>

        <%= f.submit "Save Edit"  %>
        <% end %>
<% end %>